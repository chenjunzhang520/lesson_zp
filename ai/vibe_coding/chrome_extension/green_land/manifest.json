{
  "manifest_version": 3,
  "name": "Hulk",
  "version": "1.0",
  "description": "一个可以将网页背景改为绿色的Chrome扩展",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}